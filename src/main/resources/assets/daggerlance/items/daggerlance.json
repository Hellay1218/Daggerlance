{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "gold",
        "model": {
          "type": "minecraft:select",
          "property": "minecraft:display_context",
          "cases": [
            {
              "when": [
                "ground",
                "fixed",
                "gui"
              ],
              "model": {
                "type": "minecraft:model",
                "model": "daggerlance:item/daggerlances/gold_daggerlance"
              }
            }
          ],
          "fallback": {
            "type": "minecraft:model",
            "model": "daggerlance:item/daggerlances/gold_daggerlance_handheld"
          }
        }
